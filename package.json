{
	"name": "tmp-site",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:backend\" \"npm:frontend\"",
		"frontend": "vite",
		"backend": "tsx watch server/index.ts",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"build:registry": "jsrepo build",
		"start": "npm run start:backend",
		"start:backend": "tsx server/index.ts",
		"serve": "concurrently \"npm:start:backend\" \"npm:preview\""
	},
	"dependencies": {
		"@react-three/fiber": "^9.3.0",
		"@types/express": "^5.0.3",
		"@types/nodemailer": "^6.4.17",
		"@vercel/node": "^5.3.11",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.21.2",
		"express-rate-limit": "^7.5.1",
		"geoip-lite": "^1.4.10",
		"gsap": "^3.13.0",
		"helmet": "^7.2.0",
		"motion": "^12.23.12",
		"nodemailer": "^7.0.5",
		"ogl": "^1.0.11",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^7.8.0",
		"three": "^0.167.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@types/geoip-lite": "^1.4.4",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^4.7.0",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.0",
		"eslint": "^9.32.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"jsrepo": "^2.4.5",
		"tsx": "^4.19.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.0",
		"vite": "^7.1.0"
	}
}
